{
    "manifest_version": 2,
    "name": "TabKeeper",
    "version": "1.1",
    "description": "Add multiple websites to a bookmark folder with ease",
    "permissions": ["bookmarks"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "favicon-16x16.png",
      "32": "favicon-32x32.png"
    }
  }